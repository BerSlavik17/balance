var form = $('form#for_cash')

form.attr('action', '<%= url_for :cashes %>')

form.find('input[name="_method"]').val('post')

form.dialog({
  autoOpen: true,
  modal: true,
  title: 'Добавить остаток',
  open: function() {
    $('#cash_name').val('')
    $('#cash_sum').val('0.00')
    $('#delete_cash_button').hide()
    $('#cash_deleted').attr('checked', false)
    $('#cash_errors').text('');
  }
})

