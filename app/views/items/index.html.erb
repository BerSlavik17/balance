<%= render 'form', resource: Item.new %>

<%= render 'cashes/cashes', cashes: cashes %>

<%= render 'consolidates/consolidates', consolidates: consolidates %>

<table class="items b-items">
  <thead class="header">
    <tr>
      <th class="date">Дата</th>
      <th class="sum">Сума</th>
      <th class="category">Категорія</th>
      <th>Примітка</th>
    </tr>
  </thead>

  <tbody id="items">
    <% if collection.any? %>
      <%= render collection %>
    <% else %>
      <%= render 'not_found' %>
    <% end %>
  </tbody>
</table>
