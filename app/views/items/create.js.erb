(function() {
  $('#item_form').replaceWith('<%= j render 'form', resource: Item.new %>');

  $('input.date').datepicker();

  $('#items').html('<%= j render items(DateRange.new(resource.date).month) %>');

  $('#at_end').html('<%= money Cash.at_end %>');

  $('#balance').html('<%= money Cash.balance %>');

  $('#consolidates').html('<%= j render consolidates %>');
}());
